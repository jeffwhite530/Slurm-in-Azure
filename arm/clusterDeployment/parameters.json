{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus2"
        },
        // Cluster objects will be named based on the cluster name. Use a-z, 0-9, and dashes only.
        "clusterName": {
            "value": "test-cluster"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "default-allow-ssh",
                    "properties": {
                        "priority": 500,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceApplicationSecurityGroups": [],
                        "destinationApplicationSecurityGroups": [],
                        "sourceAddressPrefix": "71.112.204.20",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "22"
                    }
                }
            ]
        },
        "addressPrefixes": {
            "value": [
                "10.200.0.0/22"
            ]
        },
        "subnets": {
            "value": [
                {
                    "name": "main",
                    "properties": {
                        "addressPrefix": "10.200.0.0/24"
                    }
                }
            ]
        },
        "headVMIsSpotInstance": {
            "value": true
        },
        "virtualMachineSourceIsMarketplace": {
            "value": false
        },
        "headVMSize": {
            "value": "Standard_E2bs_v5"
        },
        "computeVMSize": {
            "value": "Standard_E2bs_v5"
        },
        "computeVMCount": {
            "value": 1
        },
        "adminUsername": {
            "value": "jeffwhite"
        },
        "adminPublicKey": {
            "value": "ssh-rsa "
        }
    }
}